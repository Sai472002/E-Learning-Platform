{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60, 
      "memory": 1024,     
      "maxRequestSize": "200mb"  
    }
  }
}

